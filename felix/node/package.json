{
  "name": "@beyond-playground/node",
  "version": "1.0.0",
  "title": "Node Test",
  "description": "Node JS Basic application",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "distributions": [
      {
        "name": "node",
        "environment": "development",
        "platform": "node",
        "maps": "external",
        "ports": {
          "bundles": 9090,
          "inspect": 9091
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/local": "^0.1.3",
    "@beyond-js/backend": "^0.1.4"
  },
  "devDependencies": {
    "@types/node": "^16.11.7"
  }
}