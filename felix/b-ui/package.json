{
  "name": "@b-ui/ui",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "license": "ISC",
  "title": "Beyond components to create web projects",
  "languages": {
    "default": "en",
    "supported": [
      "en"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "template": "node_modules/@beyond-js/felix-tpl/template/template.json",
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "backend": "backend",
        "ports": {
          "bundles": 3500
        }
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "backend": "backend",
        "ports": {
          "bundles": 3501
        },
        "ts": {
          "compiler": "tsc"
        }
      },
      {
        "name": "backend",
        "platform": "backend",
        "host": "backend-ui.com",
        "environment": "development",
        "ports": {
          "bundles": 3501,
          "http": 3502,
          "inspect": 3503
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/local": "~0.1.3",
    "@beyond-js/backend": "~0.1.5",
    "@beyond-js/kernel": "~0.1.9",
    "@beyond-js/react-18-widgets": "~0.0.5",
    "swiper": "^8.1.4",
    "socket.io-client": "^4.5.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  }
}
