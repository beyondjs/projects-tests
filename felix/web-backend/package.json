{
	"name": "@tests/web-backend-app",
	"version": "1.0.0",
	"author": "ftovar",
	"license": "MIT",
	"title": "Basic Web-Backend Application",
	"description": "Basic Web Backend application",
	"template": "template/template.json",
	"layout": "main-layout",
	"scripts": {},
	"engines": {},
	"languages": {
		"default": "en",
		"supported": [
			"en",
			"es"
		]
	},
	"modules": {
		"path": "modules"
	},
	"deployment": {
		"distributions": [
			{
				"name": "web",
				"platform": "web",
				"environment": "development",
				"backend": "backend",
				"maps": "internal",
				"minify": {
					"css": false,
					"js": true
				},
				"ports": {
					"bundles": 5050
				}
			},
			{
				"name": "web-tsc",
				"platform": "web",
				"environment": "development",
				"backend": "backend",
				"maps": "internal",
				"minify": {
					"css": true,
					"js": true
				},
				"ports": {
					"bundles": 5051
				},
				"ts": {
					"compiler": "tsc"
				}
			},
			{
				"name": "backend",
				"platform": "backend",
				"environment": "development",
				"host": "prueba-API.com",
				"ports": {
					"bundles": 5052,
					"http": 5053,
					"inspect": 5054
				}
			}
		]
	},
	"devDependencies": {
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.9",
		"@beyond-js/local": "~0.1.3"
	},
	"dependencies": {
		"@beyond-js/backend": "~0.1.6",
		"@beyond-js/kernel": "~0.1.9",
		"@beyond-js/react-18-widgets": "~1.0.1",
		"@beyond-js/ssr": "~0.1.2",
		"@beyond-js/svelte-widgets": "~0.1.0",
		"@beyond-js/vue-widgets": "~0.1.0",
		"@beyond-js/events": "~0.0.6",
		"@beyond-js/reactive": "~0.0.1",
		"socket.io-client": "^4.5.4",
		"firebase-admin": "^11.4.1"
	},
	"static": {
		"includes": [
			"images"
		]
	}
}
