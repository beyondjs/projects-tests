{
  "scope": "tests",
  "name": "web-backend-app",
  "version": "1.0.0",
  "author": "ftovar",
  "license": "MIT",
  "title": "Basic Web-Backend Application",
  "description": "Basic Web Backend application",
  "template": "template/template.json",
  "layout": "main-layout",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "backend": "backend",
        "ports": {
          "bundles": 5050
        },
        "bundles": {
          "mode": "sjs"
        }
      },
      {
        "name": "backend",
        "platform": "backend",
        "environment": "development",
        "host": "prueba-API.com",
        "ports": {
          "bundles": 5051,
          "http": 5052,
          "inspect": 5053
        }
      }
    ]
  }
}
