{
  "name": "@playground/declarations",
  "version": "1.0.0",
  "author": "ftovar",
  "license": "MIT",
  "title": "Declarations testings",
  "description": "Declarations testings",
  "template": "template/template.json",
  "layout": "main-layout",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules"
  },
  "deployment": {
    "build": {
      "path": "../../compilaciones"
    },
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "backend": "backend",
        "maps": "internal",
        "ports": {
          "bundles": 2020
        },
        "bundles": {
          "mode": "amd"
        }
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "environment": "development",
        "backend": "backend",
        "maps": "external",
        "ports": {
          "bundles": 2021
        },
        "ts": {
          "compiler": "tsc"
        }
      },
      {
        "name": "backend",
        "platform": "backend",
        "environment": "development",
        "ports": {
          "bundles": 2022,
          "http": 2023,
          "inspect": 2024
        }
      },
      {
        "name": "backend",
        "platform": "backend",
        "environment": "development",
        "ports": {
          "bundles": 2025,
          "http": 2026,
          "inspect": 2027
        },
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  },
  "dependencies": {
    "@beyond-js/local": "~0.1.2",
    "@beyond-js/backend": "0.1.3",
    "@beyond-js/kernel": "~0.1.9",
    "@beyond-js/ssr": "~0.1.2",
    "@beyond-js/svelte-widgets": "~0.1.0",
    "@beyond-js/react-18-widgets": "~0.0.1",
    "@beyond-js/events": "~0.0.4",
    "socket.io-client": "^4.5.4",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9"
  },
  "static": {
    "includes": [
      "images"
    ]
  }
}