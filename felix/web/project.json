{
  "version": 1,
  "scope": "jida",
  "name": "web",
  "layout": "main-layout",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "params": {},
  "modules": {
    "path": "./modules",
    "externals": {}
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "port": 3508,
        "default": true
      },
      {
        "name": "web-tsc",
        "platform": "web",
        "environment": "development",
        "port": 3509,
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  },
  "static": {
    "includes": [
      "./static"
    ]
  }
}