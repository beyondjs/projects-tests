{
  "version": "1.0.0",
  "scope": "beyond-playground",
  "name": "counter-rendering",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "./modules"
  },
  "dependencies": {
    "@beyond-js/ssr": "^0.1.1",
    "@beyond-js/react-widgets": "^18.20.3"
  },
  "devDependencies": {
    "@beyond-js/local": "^0.1.0"
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "ssr": "ssr",
        "ports": {
          "bundles": 6500
        }
      },
      {
        "name": "ssr",
        "platform": "ssr",
        "environment": "development",
        "ports": {
          "bundles": 6551,
          "http": 6552,
          "inspect": 6553
        }
      }
    ]
  }
}
