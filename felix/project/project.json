{
  "version": "1.0.0",
  "scope": "test",
  "name": "project-app",
  "title": "Aplicación de pruebas",
  "description": "App con módulos básicos para pruebas en BeyondJS",
  "layout": "main-layout",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "./modules"
  },
  "static": {
    "includes": [
      "images"
    ]
  },
  "libraries": {
    "imports": [
      "@beyond-js/local",
      "@beyond-js/kernel",
      "@beyond-js/backend",
      "@beyond-js/widgets",
      "@beyond-js/react-widgets"
    ]
  },
  "cordova": {
    "id": "id app",
    "name": "name app",
    "description": "description",
    "author": {
      "name": "ftovar",
      "href": "href value",
      "email": "felix.godo"
    },
    "version": {
      "number": "1.0.0",
      "code": "1.0.0"
    }
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "platform": "web",
        "environment": "development",
        "ports": {
          "bundles": 8080
        },
        "bundles": {
          "mode": "sjs"
        },
        "maps": "internal",
        "imports": {
          "@beyond-js/kernel": "web",
          "@beyond-js/widgets": "web",
          "@beyond-js/react-widgets": "web",
          "@beyond-js/vue-widgets": "web",
          "@beyond-js/svelte-widgets": "web",
          "@beyond-js/backend": "web",
          "@beyond-js/ssr": "web"
        }
      },
      {
        "name": "android",
        "platform": "android",
        "environment": "development",
        "ports": {
          "bundles": 6060,
          "http": 6061,
          "inspect": 6062
        }
      },
      {
        "name": "web.tsc",
        "platform": "web",
        "environment": "development",
        "ports": {
          "bundles": 8085
        },
        "bundles": {
          "mode": "sjs"
        },
        "maps": "internal",
        "ts": {
          "compiler": "tsc"
        }
      }
    ]
  }
}
