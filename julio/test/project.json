{
  "version": "1.0.0",
  "scope": "julio",
  "name": "test",
  "template": "template/template.json",
  "languages": {
    "default": "en",
    "supported": [
      "en",
      "es"
    ]
  },
  "modules": {
    "path": "modules",
    "externals": {}
  },
  "libraries": {
    "imports": [
      "@beyond-js/kernel",
      "@beyond-js/widgets",
      "@beyond-js/react-widgets",
      "@beyond-js/vue-widgets",
      "@beyond-js/svelte-widgets",
      "@beyond-js/backend",
      "@beyond-js/ssr"
    ]
  },
  "deployment": {
    "distributions": [
      {
        "name": "web",
        "environment": "development",
        "platform": "web",
        "ports": {
          "bundles": 7000
        },
        "imports": {
          "@beyond-js/kernel": "web",
          "@beyond-js/widgets": "web",
          "@beyond-js/react-widgets": "web",
          "@beyond-js/vue-widgets": "web",
          "@beyond-js/svelte-widgets": "web",
          "@beyond-js/backend": "web",
          "@beyond-js/ssr": "web"
        }
      }
    ]
  }
}
